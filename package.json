{
  "name": "serverless-localstack-sns",
  "version": "0.1.0",
  "description": "Serverless plugin to run SNS off localstack for local development",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p src",
    "watch": "tsc -p src -w",
    "test": "nyc ts-mocha \"test/**/*.ts\" -p src/",
    "lint": "tslint -p src/",
    "prepare": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mj1618/serverless-localstack-sns.git"
  },
  "keywords": [
    "serverless-plugin",
    "serverless",
    "sns",
    "localstack"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Matthew James <matthew.stephen.james@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mj1618/serverless-localstack-sns/issues"
  },
  "homepage": "https://github.com/mj1618/serverless-localstack-sns#readme",
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/express": "^4.0.37",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.25",
    "aws-sdk": "^2.41.0",
    "express": "^4.15.4",
    "ts-loader": "^2.3.7",
    "ts-mocha": "^1.0.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "node-fetch": "^1.7.3",
    "nyc": "^11.2.1",
    "source-map-support": "^0.4.18"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
